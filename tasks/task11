def bmi_calculator():
    height = float(input("Enter your height in meters (e.g., 1.70): "))
    weight = float(input("Enter your weight in kilograms: "))          

    bmi = weight / (height ** 2)
    print(f"Your BMI is: {bmi:.2f}")  
    if bmi < 18.5:
        print("Underweight")         
    elif bmi < 25:
        print("Normal weight")       
    elif bmi < 30:
        print("Overweight")          
    else:
        print("Obese")               


bmi_calculator()